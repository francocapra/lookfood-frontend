<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		search="handleSearch"
		confirm="handleConfirm"
		multiSelect="true"
		cancel="handleCancel"
		items="{
			path: 'mProductsForReview>/'
		}" >
		<StandardListItem
			title="{mProductsForReview>description}"
			description="{mProductsForReview>id}"
			icon="imgs/food-tray.png"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
		</SelectDialog>
	</core:FragmentDefinition>